<h1>Sign Up</h1>

<%= form_for @user do |f| %>
    <%= f.label :name, "Name: " %>
    <%= f.text_field :name %><br>
    <%= f.label :email, "Email: " %>
    <%= f.text_field :email %><br>
    <%= f.label :password, "Password: " %>
    <%= f.text_field :password %><br>
    <br>
    <%= f.submit %>
<% end %>